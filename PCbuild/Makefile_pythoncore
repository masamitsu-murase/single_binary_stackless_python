.c.obj:
	$(CC) /c $< $(CFLAGS) $(ARCH_DEFINES) /Fo$@

.cpp.obj:
	$(CC) /c $< $(CFLAGS) $(ARCH_DEFINES) /Fo$@

.asm.obj:
	$(ASM) /Fo$@ /c $< $(AFLAGS)

clean:
	-<<delete.bat
del $(TARGET)
del $(OBJS:.obj =.obj^
del )
<<


